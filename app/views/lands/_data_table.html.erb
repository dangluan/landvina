<table class="table-basic item-list has-effect display dataTable">
  <thead>
      <th class="first">Title</th>
      <th>Vitri</th>
      <th>Tien do</th>
      <th>Gia</th>
      <th class="action last" style="text-align:center;">Actions</th>
  </thead>
  <tbody>
    <% lands.each do |land| %>
    <tr class="<%= cycle('odd', 'even') %>">
      <td><%= land.title %></td>
      <td><%= land.land_position_text %></td>
      <td><%= land.land_status %></td>
      <td><%= land.price %></td>
      <td class="actions" >
      	<i class="icon-admin-upload-preview"></i>
      	<a class="dynamic-link " data-url="/admins/lands/<%= land.id %>/upload_preview" data-element="#stage" data-method="GET" href="#">Upload preview</a>
      	<i class="icon-admin-edit"></i>
        <a class="dynamic-link " data-url="/admins/lands/<%= land.id %>/edit" data-element="#stage" data-method="GET" href="#" >Edit</a>
        <i class="icon-admin-remove"></i>
        <a class="dynamic-link " style="color: #c00;" data-url="/admins/lands/<%= land.id %>" data-element="#stage" href="#" data-method="DELETE" data-confirm="<%= "delete ?" %>">Remove</a>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>